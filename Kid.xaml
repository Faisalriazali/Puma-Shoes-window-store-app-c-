                    </Grid>
                    <TextBlock x:Name="Row1_Box1_Name" Margin="0" Grid.Row="2" TextWrapping="Wrap" Text="CARSON RUNNER CAMO KIDS RUNNING SHOES" TextAlignment="Center" Foreground="Red" FontSize="20" Padding="0,10,0,0" Tapped="Row1_Box_1_img_Tapped" ToolTipService.ToolTip="{Binding Text, ElementName=Row1_Box1_Name}"/>

