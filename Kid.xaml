<Page
    x:Name="pageRoot"
    x:Class="PumaShoes.Kid"
    DataContext="{Binding DefaultViewModel, RelativeSource={RelativeSource Self}}"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:PumaShoes"
    xmlns:common="using:PumaShoes.Common"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Page.Resources>
        <!-- TODO: Delete this line if the key AppName is declared in App.xaml -->
        <x:String x:Key="AppName">Kids Shoes</x:String>
    </Page.Resources>

    <!--
        This grid acts as a root panel for the page that defines two rows:
        * Row 0 contains the back button and page title
        * Row 1 contains the rest of the page layout
    -->
    <Grid Background="Gray" Width="{Binding ActualWidth, ElementName=pageRoot, Mode=OneWay}">
        <Grid.ChildrenTransitions>
            <TransitionCollection>
                <EntranceThemeTransition/>
            </TransitionCollection>
        </Grid.ChildrenTransitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="110"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <!-- Back button and page title -->
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="120"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Button x:Name="backButton" Content="&#xE112;" Command="{Binding NavigationHelper.GoBackCommand, ElementName=pageRoot}" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" Background="Red" BorderBrush="Black" Foreground="Black" RequestedTheme="Dark" FontFamily="Segoe UI Symbol" Padding="0" BorderThickness="2" Style="{StaticResource NavBtn}" UseLayoutRounding="True" Height="45" Width="45" FontSize="20" FontWeight="Normal" ToolTipService.ToolTip="Back"
                        AutomationProperties.Name="Back"
                        AutomationProperties.AutomationId="BackButton"
                        AutomationProperties.ItemType="Navigation Button"/>
            <TextBlock x:Name="pageTitle" Text="{StaticResource AppName}" Style="{StaticResource HeaderTextBlockStyle}" Grid.Column="1" 
                        IsHitTestVisible="false" TextWrapping="NoWrap" Margin="10,0,0,0" Foreground="Blue" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="50"/>
        </Grid>
        <ScrollViewer Grid.Row="1">
            <Grid x:Name="Main">
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <!---->
                <Grid x:Name="soccer_main" VerticalAlignment="Top" Grid.Row="0">
                    <Grid Margin="0" VerticalAlignment="Top" Background="Red">
                        <TextBlock x:Name="kid_Soccer_Toggle" VerticalAlignment="Top" Text="&#xE010; Boys &#xE010;" FontSize="25"  FontFamily="Segoe UI Symbol" Padding="25,0,0,0" Foreground="Black" TextAlignment="Center" Tapped="TextBlock_Tapped" PointerEntered="Men_Soccer_Toggle_PointerEntered" PointerExited="Men_Soccer_Toggle_PointerExited" />
                    </Grid>
                    <Grid x:Name="kids_data" Margin="0,40,0,0" Visibility="Collapsed">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="300"/>
                            <RowDefinition Height="300"/>
                            <!--<RowDefinition Height="300"/>-->
                </Grid.RowDefinitions>
                <Grid Margin="25,0" Height="300" Background="White">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="200"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Image x:Name="Row1_Box_1_img" Source="Assets/image1/image/puma kids/astral aura-astral aura-bleached denim/1.jpg" VerticalAlignment="Top" Stretch="Fill" Height="200" Tapped="Row1_Box_1_img_Tapped" ToolTipService.ToolTip="{Binding Text, ElementName=Row1_Box1_Name}"/>
                    <Grid Grid.Row="1" PointerExited="Grid_PointerExited" Background="White">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Rectangle Fill="Red" Height="3" Margin="10,0,10,9" Stroke="Black" VerticalAlignment="Bottom" StrokeThickness="0" />
                        <Image x:Name="Row1_Box_1_color1" Source="Assets/image1/image/puma kids/astral aura-astral aura-bleached denim/358854_02_SWT_PNA.jpg" VerticalAlignment="Top" Stretch="Fill" Height="18" Grid.Column="0" Margin="10,0,10,5" PointerEntered="color1_PointerEntered" Tapped="Row1_Box_1_img_Tapped"/>
                        <Image x:Name="Row1_Box_1_color2" Source="Assets/image1/image/puma kids/black-limestone gray/359043_01_SWT_PNA.jpg" VerticalAlignment="Top" Stretch="Fill" Height="18" Grid.Column="1" Margin="10,0,10,5" PointerEntered="color2_PointerEntered" Tapped="Row1_Box_2_img_Tapped"/>
