                        <RowDefinition Height="200"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Image x:Name="Row2_Box_4_img" Source="Assets/image1/image/puma women shoes/women running/fluo peach-white/1.jpg" VerticalAlignment="Top" Stretch="Fill" Height="200" Tapped="Row2_Box_4_img_Tapped" ToolTipService.ToolTip="{Binding Text, ElementName=textBlock6}"/>
                    <Grid Grid.Row="1" PointerExited="Grid7_PointerExited" Background="White">
