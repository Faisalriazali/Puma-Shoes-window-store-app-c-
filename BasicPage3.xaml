    <Grid x:Name="two_soccer_main" VerticalAlignment="Top" Grid.Row="1">
        <Grid Margin="0" VerticalAlignment="Top" Background="Green">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="371*"/>
