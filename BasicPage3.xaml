                        <Image x:Name="Row1_Box_3_color1" Source="Assets/image1/image/puma women shoes/women running/black/black-white.jpg" VerticalAlignment="Top" Stretch="Fill" Height="18" Grid.Column="0" Margin="10,0,10,5" PointerEntered="Row1_Box_3_color1_PointerEntered" Tapped="Row1_Box_1_img_Tapped"/>
                        <Image x:Name="Row1_Box_3_color2" Source="Assets/image1/image/puma women shoes/women running/black-purple cactus flower/357640_15_SWT_PNA.jpg" VerticalAlignment="Top" Stretch="Fill" Height="18" Grid.Column="1" Margin="10,0,10,5" PointerEntered="Row1_Box_3_color2_PointerEntered" Tapped="Row1_Box_2_img_Tapped"/>
                        <Image x:Name="Row1_Box_3_color3" Source="Assets/image1/image/puma women shoes/women running/bleached denim-astral aura-cayenne/188533_01_SWT_PNA.jpg" VerticalAlignment="Top" Stretch="Fill" Height="18" Grid.Column="2" Margin="10,0,10,5" PointerEntered="Row1_Box_3_color3_PointerEntered" Tapped="Row1_Box_3_img_Tapped"/>

                    </Grid>
                    <TextBlock x:Name="textBlock1" Margin="0" Grid.Row="2" TextWrapping="Wrap" Text="IGNITE ULTIMATE WOMEN'S RUNNING SHOES " TextAlignment="Center" Foreground="Red" FontSize="20" Padding="0,10,0,0" Tapped="Row1_Box_3_img_Tapped" ToolTipService.ToolTip="{Binding Text, ElementName=textBlock1}"/>

                </Grid>
                <Grid Margin="25,0" Height="300" Background="White" Grid.Column="3">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="200"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Image x:Name="Row1_Box_4_img" Source="Assets/image1/image/puma women shoes/women running/clearwater-blue coral-sulphur spring/1.jpg" VerticalAlignment="Top" Stretch="Fill" Height="200" Tapped="Row1_Box_4_img_Tapped" ToolTipService.ToolTip="{Binding Text, ElementName=textBlock2}"/>
                    <Grid Grid.Row="1" Background="White">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Rectangle Fill="Red" Height="3" Margin="10,0,10,9" Stroke="Black" VerticalAlignment="Bottom" StrokeThickness="0" Grid.Column="1" />
                        <Image x:Name="Row1_Box_4_color1" Source="Assets/image1/image/puma women shoes/women running/clearwater-blue coral-sulphur spring/light_blue_Yellow.jpg" VerticalAlignment="Top" Stretch="Fill" Height="18" Grid.Column="1" Margin="10,0,10,5" Tapped="Row1_Box_4_img_Tapped"/>


                    </Grid>
                    <TextBlock x:Name="textBlock2" Margin="0" Grid.Row="2" TextWrapping="Wrap" Text="IGNITE ULTIMATE WOMEN'S RUNNING SHOES "  TextAlignment="Center" Foreground="Red" FontSize="20" Padding="0,10,0,0" Tapped="Row1_Box_4_img_Tapped" ToolTipService.ToolTip="{Binding Text, ElementName=textBlock2}" />

                </Grid>


                <Grid Margin="25,0" Height="300" Background="White" Grid.Row="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="200"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Image x:Name="Row2_Box_1_img" Source="Assets/image1/image/puma women shoes/women running/fiery coral-black-periscope/1.jpg" VerticalAlignment="Top" Stretch="Fill" Height="200" Tapped="Row2_Box_1_img_Tapped" ToolTipService.ToolTip="{Binding Text, ElementName=textBlock3}" />
                    <Grid Grid.Row="1" PointerExited="Grid4_PointerExited" Background="White">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Rectangle Fill="Red" Height="3" Margin="10,0,10,9" Stroke="Black" VerticalAlignment="Bottom" StrokeThickness="0" />
                        <Image x:Name="Row2_Box_1_color1" Source="Assets/image1/image/puma women shoes/women running/fiery coral-black-periscope/Black-Red-White.jpg" Stretch="Fill" Grid.Column="0" Margin="10,0,10,5" PointerEntered="Row2_Box_1_color1_PointerEntered" VerticalAlignment="Top" Height="18" Tapped="Row2_Box_1_img_Tapped"/>
                        <Image x:Name="Row2_Box_1_color2" Source="Assets/image1/image/puma women shoes/women running/fiery coral-cayenne-puma silver/188445_02_SWT_PNA.jpg" VerticalAlignment="Top" Stretch="Fill" Height="18" Grid.Column="1" Margin="10,0,10,5" PointerEntered="Row2_Box_1_color2_PointerEntered" Tapped="Row2_Box_2_img_Tapped"/>
                        <Image x:Name="Row2_Box_1_color3" Source="Assets/image1/image/puma women shoes/women running/fluo peach-rose red/pink-White.jpg" VerticalAlignment="Top" Stretch="Fill" Height="18" Grid.Column="2" Margin="10,0,10,5" PointerEntered="Row2_Box_1_color3_PointerEntered" Tapped="Row2_Box_3_img_Tapped"/>
                        <Image x:Name="Row2_Box_1_color4" Source="Assets/image1/image/puma women shoes/women running/fluo peach-white/Pink-White.jpg" VerticalAlignment="Top" Stretch="Fill" Height="18" Grid.Column="3" Margin="10,0,10,5" PointerEntered="Row2_Box_1_color4_PointerEntered" Tapped="Row2_Box_4_img_Tapped"/>
                    </Grid>
                    <TextBlock x:Name="textBlock3" Margin="0" Grid.Row="2" TextWrapping="Wrap" Text="IGNITE ULTIMATE WOMEN'S RUNNING SHOES " TextAlignment="Center" Foreground="Red" FontSize="20" Padding="0,10,0,0" Tapped="Row2_Box_1_img_Tapped" ToolTipService.ToolTip="{Binding Text, ElementName=textBlock3}"/>

                </Grid>
                <Grid Margin="25,0" Height="300" Background="White" Grid.Column="1" Grid.Row="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="200"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Image x:Name="Row2_Box_2_img" Source="Assets/image1/image/puma women shoes/women running/fiery coral-cayenne-puma silver/1.jpg" VerticalAlignment="Top" Stretch="Fill" Height="200" Tapped="Row2_Box_2_img_Tapped" ToolTipService.ToolTip="{Binding Text, ElementName=textBlock4}"/>
                    <Grid Grid.Row="1" PointerExited="Grid5_PointerExited" Background="White">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Rectangle Fill="Red" Height="3" Margin="10,0,10,9" Stroke="Black" VerticalAlignment="Bottom" StrokeThickness="0" Grid.Column="1" />
                        <Image x:Name="Row2_Box_2_color1" Source="Assets/image1/image/puma women shoes/women running/fiery coral-black-periscope/Black-Red-White.jpg" Stretch="Fill" Grid.Column="0" Margin="10,0,10,5" PointerEntered="Row2_Box_2_color1_PointerEntered" VerticalAlignment="Top" Height="18" Tapped="Row2_Box_1_img_Tapped" />
                        <Image x:Name="Row2_Box_2_color2" Source="Assets/image1/image/puma women shoes/women running/fiery coral-cayenne-puma silver/188445_02_SWT_PNA.jpg" Stretch="Fill" Grid.Column="1" Margin="10,0,10,5" PointerEntered="Row2_Box_2_color2_PointerEntered" VerticalAlignment="Top" Height="18" Tapped="Row2_Box_2_img_Tapped"/>
                        <Image x:Name="Row2_Box_2_color3" Source="Assets/image1/image/puma women shoes/women running/fluo peach-rose red/pink-White.jpg" VerticalAlignment="Top" Stretch="Fill" Height="18" Grid.Column="2" Margin="10,0,10,5" PointerEntered="Row2_Box_2_color3_PointerEntered" Tapped="Row2_Box_3_img_Tapped"/>
                        <Image x:Name="Row2_Box_2_color4" Source="Assets/image1/image/puma women shoes/women running/fluo peach-white/Pink-White.jpg" VerticalAlignment="Top" Stretch="Fill" Height="18" Grid.Column="3" Margin="10,0,10,5" PointerEntered="Row2_Box_2_color4_PointerEntered" Tapped="Row2_Box_4_img_Tapped"/>

                    </Grid>
                    <TextBlock x:Name="textBlock4" Margin="0" Grid.Row="2" TextWrapping="Wrap" Text="IGNITE ULTIMATE MULTI WOMEN'S RUNNING SHOES " TextAlignment="Center" Foreground="Red" FontSize="20" Padding="0,10,0,0" Tapped="Row2_Box_2_img_Tapped" ToolTipService.ToolTip="{Binding Text, ElementName=textBlock4}"/>

                </Grid>
                <Grid Margin="25,0" Height="300" Background="White" Grid.Column="2" Grid.Row="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="200"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Image x:Name="Row2_Box_3_img" Source="Assets/image1/image/puma women shoes/women running/fluo peach-rose red/1.jpg" VerticalAlignment="Top" Stretch="Fill" Height="200" Tapped="Row2_Box_3_img_Tapped" ToolTipService.ToolTip="{Binding Text, ElementName=textBlock5}"/>
                    <Grid Grid.Row="1" PointerExited="Grid6_PointerExited" Background="White">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Rectangle Fill="Red" Height="3" Margin="10,0,10,9" Stroke="Black" VerticalAlignment="Bottom" StrokeThickness="0" Grid.Column="2" />
                        <Image x:Name="Row2_Box_3_color1" Source="Assets/image1/image/puma women shoes/women running/fiery coral-black-periscope/Black-Red-White.jpg" Stretch="Fill" Grid.Column="0" Margin="10,0,10,12" PointerEntered="Row2_Box_3_color1_PointerEntered" Tapped="Row2_Box_1_img_Tapped" />
