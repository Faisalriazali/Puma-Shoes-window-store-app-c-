                        <Image x:Name="Row2_Box_4_color2" Source="Assets/image1/image/puma women shoes/women running/fiery coral-cayenne-puma silver/188445_02_SWT_PNA.jpg" VerticalAlignment="Top" Stretch="Fill" Height="18" Grid.Column="1" Margin="10,0,10,5" PointerEntered="Row2_Box_4_color2_PointerEntered" Tapped="Row2_Box_2_img_Tapped"/>
                        <Image x:Name="Row2_Box_4_color3" Source="Assets/image1/image/puma women shoes/women running/fluo peach-rose red/pink-White.jpg" VerticalAlignment="Top" Stretch="Fill" Height="18" Grid.Column="2" Margin="10,0,10,5" PointerEntered="Row2_Box_4_color3_PointerEntered" Tapped="Row2_Box_3_img_Tapped"/>
