                        <Image x:Name="Row2_Box_3_color4" Source="Assets/image1/image/puma women shoes/women running/fluo peach-white/Pink-White.jpg" VerticalAlignment="Top" Stretch="Fill" Height="18" Grid.Column="3" Margin="10,0,10,5" PointerEntered="Row2_Box_3_color4_PointerEntered" Tapped="Row2_Box_4_img_Tapped"/>

                    </Grid>
                    <TextBlock x:Name="textBlock5" Margin="0" Grid.Row="2" TextWrapping="Wrap" Text="IGNITE ULTIMATE 3D WOMEN'S RUNNING SHOES " TextAlignment="Center" Foreground="Red" FontSize="20" Padding="0,10,0,0" Tapped="Row2_Box_3_img_Tapped" ToolTipService.ToolTip="{Binding Text, ElementName=textBlock5}"/>

                </Grid>
                <Grid Margin="25,0" Height="300" Background="White" Grid.Column="3" Grid.Row="1">
